{
  "name": "lerna-staudy",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "lerna": "^3.16.4",
    "rollup-plugin-clear": "^2.0.7"
  },
  "dependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-syntax-async-generators": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "father-build": "^1.18.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-react-svg": "^3.0.3",
    "rollup-plugin-sass": "^1.2.2",
    "typescript": "^3.9.6"
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "precommit": "lint-staged",
    "test": "jest --projects ./packages/*/",
    "prettier": "prettier --write \"src/**/*.jsx\" \"src/**/*.js\"",
    "lint": "eslint src/**/*.js",
    "publish-l": "lerna publish",
    "build": "PACKAGE=house father-build",
    "build:rollup": "lerna exec --parallel -- rollup -f esm -i src/index.js -o dist/index.js",
    "build:lerna": "lerna run build"
  }
}
