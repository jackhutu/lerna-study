{
  "name": "lerna-staudy",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "lerna": "^3.16.4"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^3.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "father-build": "^1.18.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^3.9.6"
  },
  "lint-staged": {
    "packages/**/*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "precommit": "lint-staged",
    "test": "jest --projects ./packages/*/",
    "prettier": "prettier --write \"src/**/*.jsx\" \"src/**/*.js\"",
    "lint": "eslint src/**/*.js",
    "publish-l": "lerna publish",
    "build": "PACKAGE=house father-build",
    "build:rollup": "lerna exec --parallel -- rollup -f esm -i src/index.js -o dist/index.js"
  }
}
